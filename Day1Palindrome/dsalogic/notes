let s = "govind";
let n =s.length;
 n = Math.floor(n/2)
str1 = s.toLowerCase();
let {log} = console

let bool = true;
for(let i = 0; i<n;i++){
    if(str1[i] !=str1[str1.length - i - 1]  )
    {
      bool = false;
      break;
    }
}

if(bool){
    log("yes")
}else{
    log("no")
}



